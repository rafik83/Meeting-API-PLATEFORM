{% extends "admin/base.html.twig" %}

{% block body %}
    <div class="container grid px-6 mx-auto">
        {% include 'admin/common/return_button.html.twig' with { link: path('admin_nomenclature_list') } %}

        <h2 class="part-title">{{ 'admin.nomenclature.create'|trans }}</h2>
        <div class="main-rounded-block">
            {{ form_start(form) }}
            {{ form_row(form.community) }}
            {{ form_row(form.reference) }}
            <button type="submit" class="btn-medium">{{ 'admin.actions.create'|trans }}</button>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}
